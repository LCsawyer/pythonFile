# -*- coding: UTF-8 -*-
import scene.Scene
from sys import exit
class death(Scene):
	def enter(self):
		print "you are dead"
		exit(1)


class output(Scene):
	def enter(self):
		print "you win!"
		exit(0)

class in_put(Scene):
	def enter(self):
		print "please choice the pattern"
		print "please enter the easy , medium,hard and orthers"
		choice = raw_input('>')
		if choice == 'easy'
			return 'easy'

class medium(object) :
	def enter(self):
		print "you should guess 1 to 5"
		gdata = 4
		guess = raw_input('input>')
		
		if int(guess) != gdata :
			return 'death'
		else :
			print "go through---------------"
			print "vampire here"
			
			goth = raw_input('chioce>')
			
			if goth == 'fight':
				return 'death'
			elif goth == 'blood':
				return 'output'
			else :
				return 'death'
